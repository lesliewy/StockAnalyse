      <color>#FE3EE1</color>
      <selected></selected>                     
      <balloon_text>
        <![CDATA[主力净流入:{value}万元]]>
      </balloon_text>
    </graph>

    <graph gid="1">
      <axis>left</axis>
      <title>超大单净流入</title>
      <color>#650000</color>
      <selected></selected>
      <balloon_text>
        <![CDATA[超大单净流入:{value}万元]]>
      </balloon_text>
    </graph>

    <graph gid="2">                                                            
